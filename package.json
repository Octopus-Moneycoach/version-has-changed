{
  "name": "has-version-changed",
  "version": "1.0.3",
  "private": true,
  "description": "TypeScript template action",
  "main": "lib/main.js",
  "scripts": {
    "build": "tsc",
    "format": "prettier --write '**/*.ts'",
    "format-check": "prettier --check '**/*.ts'",
    "lint": "eslint src/**/*.ts",
    "package": "ncc build --source-map --license licenses.txt",
    "patch": "npm version patch",
    "commit": "git commit -a -m 'Automated'",
    "checkout-to-release": "git checkout release/v1",
    "checkout-to-main": "git checkout main",
    "rebase": "git rebase main",
    "push": "git push ",
    "deploy": "npm run build && npm run format && npm run lint && npm run package && npm run patch --force && npm run commit && npm run push"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Octopus-Moneycoach/version-has-changed.git"
  },
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "^1.11.1",
    "@actions/exec": "^1.1.1",
    "wildcard-match": "^5.1.4"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.33.0",
    "@stylistic/eslint-plugin": "^5.2.3",
    "@types/node": "^24.3.0",
    "@typescript-eslint/eslint-plugin": "^8.39.1",
    "@typescript-eslint/parser": "^8.39.1",
    "@vercel/ncc": "^0.38.3",
    "eslint": "^9.33.0",
    "eslint-plugin-github": "^6.0.0",
    "eslint-plugin-jest": "^29.0.1",
    "eslint-plugin-prettier": "^5.5.4",
    "globals": "^16.3.0",
    "jest": "^30.0.5",
    "js-yaml": "^4.1.0",
    "prettier": "^3.6.2",
    "ts-jest": "^29.4.1",
    "typescript": "^5.9.2"
  }
}
